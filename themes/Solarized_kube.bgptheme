override_git_prompt_colors() {
  GIT_PROMPT_THEME_NAME="Solarized_kube"
  GIT_PROMPT_STAGED="${Yellow}●"
  GIT_PROMPT_STASHED="${BoldMagenta}⚑ "
  GIT_PROMPT_CLEAN="${Green}✔"
  GIT_PROMPT_END_USER=" \n${BoldBlue}${Time12a}${ResetColor} \$(kube_ps1) $ "
  GIT_PROMPT_END_ROOT=" \n${BoldBlue}${Time12a}${ResetColor} \$(kube_ps1) # "
}
reload_git_prompt_colors "Solarized_kube"
